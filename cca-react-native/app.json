{
  "expo": {
    "name": "CleanChat",
    "slug": "cca-react-native",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/chat-icon-generic.png",
    "splash": {
      "image": "./assets/splash-cca-generic.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.keenmaster486.cleanchat"
    }
  }
}
